<%= simple_form_for :billing, :url => url_for(:action => 'create', :controller => 'billings'),
    :method => 'post' do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :start, as: :text %>
  </div>

  <div class="form-actions">
    <%= f.button :submit, "Bill Members" %>
  </div>
<% end %>
